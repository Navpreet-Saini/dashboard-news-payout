{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "typeRoots": ["./types", "./node_modules/@types"],
    "incremental": true,
    // "types": ["next-auth"],
    "baseUrl": ".", 
    
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "yes/*": ["./src/*"],
      "@/*": ["./src/*"]
    }
  },
  "include": ["src/next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts","next-env.d.ts", "**/*.ts", "**/*.tsx", "src/types", "tailwind.config.js"],
  "exclude": ["node_modules"]
}
